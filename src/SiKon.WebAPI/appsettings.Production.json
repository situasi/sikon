{
  "Serilog":
  {
    "Using":
    [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel":
    {
      "Default": "Information"
    },
    "WriteTo":
    [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args":
        {
          "serverUrl": "http://localhost:5341"
        }
      }
    ],
    "Enrich":
    [
      "FromLogContext",
      "WithMachineName"
    ],
    "Properties":
    {
      "ApplicationName": "SiKon API"
    }
  },
  "Hangfire":
  {
    "Database": "Data Source=localhost;Initial Catalog=SharePointSyncDB;Persist Security Info=True;User ID=sharepoint;Password=P@ss123",
    "Dashboard":
    {
      "Url": "/jobs",
      "Username": "admin",
      "Password": "admin"
    }
  }
}